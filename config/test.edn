{:web-server 
 {:port #long #env PORT
  :host #env HOST}
 :jwt-secret #env JWT_SECRET
 :email {:host   #env EMAIL_HOST
         :port   #long #env EMAIL_PORT
         :user   #env EMAIL_USER
         :pass   #env EMAIL_PASS
         :from   #env EMAIL_FROM
         :tls    true}
 :db {:spec {:dbtype "postgresql"
              :host #env DB_HOST
              :port #long #env DB_PORT
              :dbname #env DB_NAME
              :user #env DB_USER
              :password #env DB_PASS}}
 :s3 {:bucket "your-test-bucket"
      :region "us-east-1"
      :access-key-id "your-access-key-id"
      :secret-access-key "your-secret-access-key"}}
